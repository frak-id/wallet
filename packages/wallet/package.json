    "cuer": "^0.0.2",
    "dexie": "^4.0.11",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.0.5",
    "isbot": "^5.1.27",
    "jose": "^6.0.10",
    "jotai": "^2.12.3",
    "lucide-react": "^0.503.0",
    "micromark": "^4.0.2",
