{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["es2023", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "verbatimModuleSyntax": true
  },
  "include": [
    "./next-env.d.ts", "**/*.ts"],
  "exclude": ["node_modules"]
}
