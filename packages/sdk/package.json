{
  "name": "@frak-labs/nexus-sdk",
  "author": "Frak Labs",
  "maintainers": [
    {
      "name": "srod",
      "url": "https://twitter.com/srod"
    },
    {
      "name": "Quentin Nivelais",
      "url": "https://twitter.com/QNivelais"
    }
  ],
  "version": "0.0.4",
  "description": "Nexus Wallet client SDK, helping any person to interact with the Frak wallet, and require the unlock of a premium article within the Frak ecosystem.",
  "repository": "https://github.com/frak-id/wallet",
  "homepage": "https://docs.frak.id/wallet-sdk",
  "keywords": [
    "frak-labs",
    "nexus-wallet",
    "erc-4337",
    "eip-4337",
    "smart-wallet"
  ],
  "license": "GNU GPL 3.0",
  "sideEffects": false,
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "default": "./dist/core/index.cjs"
    },
    "./actions": {
      "types": "./dist/core/actions/index.d.ts",
      "import": "./dist/core/actions/index.js",
      "default": "./dist/core/actions/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "default": "./dist/react/index.cjs"
    }
  },
  "scripts": {
    "lint": "biome lint .",
    "format:check": "biome check .",
    "format": "biome check --apply .",
    "clean": "rimraf dist",
    "build": "tsup --splitting"
  },
  "peerDependencies": {
    "viem": "^2.x",
    "@tanstack/react-query": ">=5.0.0",
    "react": ">=18"
  },
  "dependencies": {
    "async-lz-string": "^1.1.0",
    "js-sha256": "^0.11.0"
  },
  "devDependencies": {
    "@tanstack/react-query": ">=5.28.6",
    "@types/node": "^20",
    "tsup": "^8.0.2",
    "typescript": "^5"
  }
}