{
  "name": "@frak-labs/backend-elysia",
  "description": "Elysia Backend for Frak",
  "version": "0.0.10",
  "private": true,
  "type": "module",
  "license": "GNU GPL 3.0",
  "scripts": {
    "lint": "biome lint .",
    "format:check": "biome check .",
    "format": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "dev": "sst bind bun dev:watch",
    "dev:docker": "sst bind \"env | grep -E 'SST_|AWS_' > .env.tmp && docker run --env-file .env.tmp -P elysia-dev\"",
    "build:docker": "docker build --tag \"elysia-dev\" .",
    "dev:watch": "bun run --watch src/index.ts",
    "build": "bun build:package",
    "build:package": "bun build --minify --target bun --outfile ./dist/index.js src/index.ts",
    "build:binary": "bun build --compile --minify --target bun --outfile ./server src/index.ts",
    "start": "NODE_ENV=production bun dist/index.js",
    "db:studio": "sst bind drizzle-kit studio",
    "db:generate": "sst bind drizzle-kit generate",
    "db:migrate": "sst bind drizzle-kit migrate"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.679.0",
    "@aws-sdk/client-secrets-manager": "^3.674.0",
    "@bogeychan/elysia-logger": "^0.1.4",
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/cron": "^1.1.1",
    "@frak-labs/app-essentials": "workspace:*",
    "@frak-labs/nexus-sdk": "workspace:*",
    "@simplewebauthn/server": "^11.0.0",
    "@tusbar/cache-control": "^1.0.2",
    "async-mutex": "^0.5.0",
    "@sinclair/typebox": "0.32.34",
    "croner": "^9.0.0",
    "drizzle-orm": "^0.34.1",
    "elysia": "1.1.23",
    "iron-session": "^8.0.3",
    "jose": "^5.9.6",
    "ky": "^1.7.2",
    "lru-cache": "^11.0.1",
    "merkletreejs": "^0.4.0",
    "mongodb": "^6.9.0",
    "permissionless": "^0.2.11",
    "postgres": "^3.4.5",
    "radash": "^12.1.0",
    "solady": "^0.0.259",
    "sst": "2.44.0",
    "viem": "^2.21.35",
    "web-push": "^3.6.7",
    "whatwg-url": "^14.0.0"
  },
  "devDependencies": {
    "@simplewebauthn/types": "^11.0.0",
    "@types/web-push": "^3.6.3",
    "bun-types": "latest",
    "drizzle-kit": "^0.25.0"
  },
  "module": "src/index.ts",
  "main": "./src/index.ts"
}