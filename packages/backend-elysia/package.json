{
  "name": "@frak-labs/backend-elysia",
  "description": "Backend for Frak using Elysia",
  "version": "0.0.13",
  "private": true,
  "type": "module",
  "license": "GNU GPL 3.0",
  "scripts": {
    "lint": "biome lint .",
    "format:check": "biome check .",
    "format": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "dev": "bun sst dev bun dev:watch",
    "dev:env": "sst shell bun run scripts/envSetup.ts",
    "dev:watch": "bun run --watch src/index.ts",
    "build:docker": "docker build --tag \"elysia-dev\" .",
    "build": "bun build:package",
    "build:package": "bun run build.ts",
    "build:binary-linux-arm64": "bun build --compile --minify --target=bun-linux-arm64 --outfile ./server src/index.ts",
    "build:binary": "bun build --compile --minify --target=bun --outfile ./server src/index.ts",
    "start": "NODE_ENV=production bun dist/index-terser.js",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.772.0",
    "@bogeychan/elysia-logger": "^0.1.8",
    "@elysiajs/cors": "^1.2.0",
    "@frak-labs/app-essentials": "workspace:*",
    "@frak-labs/core-sdk": "workspace:*",
    "@simplewebauthn/server": "^13.1.1",
    "@sinclair/typebox": "0.34.31",
    "@tusbar/cache-control": "^1.0.2",
    "async-mutex": "^0.5.0",
    "croner": "^9.0.0",
    "drizzle-orm": "^0.41.0",
    "elysia": "1.2.25",
    "iron-session": "^8.0.4",
    "jose": "^6.0.10",
    "ky": "^1.7.5",
    "lru-cache": "^11.1.0",
    "merkletreejs": "^0.5.1",
    "mongodb": "^6.15.0",
    "permissionless": "^0.2.38",
    "postgres": "^3.4.5",
    "radash": "^12.1.0",
    "solady": "^0.1.12",
    "sst": "3.9.33",
    "viem": "^2.23.14",
    "web-push": "^3.6.7",
    "whatwg-url": "^14.2.0"
  },
  "devDependencies": {
    "@types/web-push": "^3.6.4",
    "@types/whatwg-url": "^13.0.0",
    "bun-types": "latest",
    "drizzle-kit": "^0.30.5",
    "terser": "^5.39.0"
  },
  "module": "src/index.ts",
  "main": "./src/index.ts"
}