    "@frak-labs/frame-connector": "workspace:*",
    "@frak-labs/ui": "workspace:*",
    "@frak-labs/wallet-shared": "workspace:*",
    "@khmyznikov/pwa-install": "^0.5.8",
    "@openpanel/web": "^1.0.1",
    "ox": "^0.9.14",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-router/node": "^7.9.5",
    "@react-router/serve": "^7.9.5",
